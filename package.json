{
  "name": "webpack-mpa-template",
  "author": "Broccoli spring( gcx ) <Lensgcx@163.com>",
  "version": "1.0.1",
  "description": "webpack mpa template ( webpack 的多页面项目构建示例 )",
  "main": "dist/index.html",
  "scripts": {
    "test": "npm run build --[test]",
    "dev": "cross-env NODE_ENV=development webpack-dev-server  --config build/webpack.dev.conf.js",
    "build": "cross-env NODE_ENV=production node build/build.js",
    "build2": "cross-env NODE_ENV=production webpack --config build/webpack.prod.conf.js",
    "server": "live-server ./ --port=8888",
    "analyz": "NODE_ENV=production npm_config_report=true npm run build"
  },
  "keywords": [
    "webpack",
    "mpa",
    "frontend",
    "javascript",
    "es6"
